            <!--<field name="res_model">renewal.again</field>-->
            <!--<field name="view_mode">tree,form,search</field>-->
            <!--<field name="view_type">form</field>-->
        <!--</record>-->
        <record id="my_action_for_second_menu_renewal_action" model="ir.actions.act_window">
            <field name="name">renewal</field>
            <field name="res_model">renewal.again</field>
            <field name="view_mode">tree,form,search</field>
            <field name="view_type">form</field>
        </record>

    </data>
</odoo>